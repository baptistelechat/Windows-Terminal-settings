{
    "$help": "https://aka.ms/terminal-documentation",
    "$schema": "https://aka.ms/terminal-profiles-schema",
    "actions": 
    [
        {
            "command": 
            {
                "action": "splitPane",
                "split": "right",
                "splitMode": "duplicate"
            },
            "keys": "alt+shift+v"
        },
        {
            "command": 
            {
                "action": "copy",
                "singleLine": false
            },
            "keys": "ctrl+c"
        },
        {
            "command": "togglePaneZoom",
            "keys": "ctrl+alt+space"
        },
        {
            "command": "paste",
            "keys": "ctrl+v"
        },
        {
            "command": 
            {
                "action": "newTab"
            },
            "keys": "ctrl+t"
        },
        {
            "command": "openTabRenamer",
            "keys": "f2"
        },
        {
            "command": "find",
            "keys": "ctrl+f"
        },
        {
            "command": "tabSearch",
            "keys": "ctrl+shift+f"
        },
        {
            "command": "openTabColorPicker",
            "keys": "f3"
        },
        {
            "command": 
            {
                "action": "splitPane",
                "split": "auto",
                "splitMode": "duplicate"
            },
            "keys": "alt+shift+d"
        },
        {
            "command": 
            {
                "action": "splitPane",
                "split": "down",
                "splitMode": "duplicate"
            },
            "keys": "alt+shift+h"
        },
        {
            "command": 
            {
                "action": "newWindow"
            },
            "keys": "ctrl+shift+n"
        }
    ],
